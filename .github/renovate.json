{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^version$"],
      "matchStrings": ["^(?<currentValue>.*?)$"],
      "depNameTemplate": "arp242/goatcounter",
      "datasourceTemplate": "github-releases"
    }
  ]
}
